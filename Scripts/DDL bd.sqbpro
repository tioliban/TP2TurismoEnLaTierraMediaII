	PRIMARY KEY(&quot;nombreUsuario&quot;),
	FOREIGN KEY(&quot;nombreTipo&quot;) REFERENCES &quot;tipoAtraccion&quot;(nombreTipo)
);
CREATE TABLE &quot;itinerarios&quot; (
	&quot;idItinerario&quot;	INTEGER NOT NULL UNIQUE,
	&quot;nombreUsuario&quot;	TEXT NOT NULL,
	&quot;nombrePromocion&quot;	TEXT,
