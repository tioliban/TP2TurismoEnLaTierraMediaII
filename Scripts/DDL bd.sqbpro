);
CREATE TABLE &quot;atracciones&quot; (
	&quot;nombreAtraccion&quot;	TEXT NOT NULL UNIQUE,
	&quot;tiempo&quot;	REAL NOT NULL,
	&quot;costo&quot;	REAL NOT NULL,
	&quot;cupo&quot;	INTEGER NOT NULL,
	&quot;nombreTipo&quot;	TEXT NOT NULL,
